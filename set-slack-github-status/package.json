{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "set-slack-github-status",
  "title": "Set Slack + Github Status",
  "description": "Set your status on Slack and Github simultaneously. ",
  "icon": "command-icon.png",
  "author": "sethrubenstein",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "status",
      "title": "Status",
      "description": "Set your Slack + Github status",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@raycast/api": "^1.63.0",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
